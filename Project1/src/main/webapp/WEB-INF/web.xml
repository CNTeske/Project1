<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>ERSServlet</servlet-name>
		<servlet-class>com.revature.servlets.ERSServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ViewServlet</servlet-name>
		<servlet-class>com.revature.servlets.ViewServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>SessionServlet</servlet-name>
		<servlet-class>com.revature.servlets.SessionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>S3Upload</servlet-name>
		<display-name>S3Upload</display-name>
		<description></description>
		<servlet-class>com.revature.servlets.S3Upload</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ViewServlet</servlet-name>
		<url-pattern>/view</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>SessionServlet</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ERSServlet</servlet-name>
		<url-pattern>/ers</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>S3Upload</servlet-name>
		<url-pattern>/S3Upload</url-pattern>
	</servlet-mapping>


</web-app>
